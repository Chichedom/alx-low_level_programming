#include "main.h"
#include <stdio.h>

/**
 * times_table - this function prints the  9 x 9 times table, starting at 0.
 * Return: the 9 x 9 times table  
*/

void times_table(void)
  
{
int num, col, prod;
for (num = 0; num <= 9; num++)    
putchar('0');      
for (col = 1; col <= 9; col++)
putchar(',');
putchar(' ');
prod = num * mult;
if (prod <= 9)	    
putchar(' ');
else
putchar((prod / 10) + '0');	  
putchar((prod % 10) + '0');     
putchar('\n');  
}
